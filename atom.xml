---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Chris Brough</title>
  <link href="http://chrisbrough.com/"/>
  <link type="application/atom+xml" rel="self" href="http://chrisbrough.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://chrisbrough.com/</id>
  <author>
    <name>Chris Brough</name>
    <email>chrisbrough@me.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://chrisbrough.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://chrisbrough.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Chris Brough</name>
      <uri>http://chrisbrough.com/</uri>
    </author>
    <content type="html"></content>
  </entry>
  {% endfor %}

</feed>
